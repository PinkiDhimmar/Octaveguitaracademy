<!DOCTYPE html>
<html>
<head>
  <title>Manage Students</title>
</head>
<body>
  <h1>Student List</h1>
  <table border="1" cellpadding="8">
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
    </tr>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
      </tr>
    <% }) %>
  </table>

  <br>
  <a href="/admin_dashboard">Back to Dashboard</a>
</body>
</html>
